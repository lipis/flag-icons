{
  "author": "Panayiotis Lipiridis <lipiridis@gmail.com>",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "bower": "1.8.2",
    "browser-sync": "2.23.6",
    "coffee-script": "1.12.7",
    "del": "3.0.0",
    "eslint": "4.17.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.1.0",
    "gulp-babel": "7.0.1",
    "gulp-bower": "0.0.14",
    "gulp-coffee": "3.0.2",
    "gulp-concat": "2.6.1",
    "gulp-cssnano": "2.1.2",
    "gulp-help": "1.6.1",
    "gulp-if": "2.0.2",
    "gulp-less": "3.4.0",
    "gulp-load-plugins": "1.5.0",
    "gulp-plumber": "1.2.0",
    "gulp-sequence": "1.0.0",
    "gulp-size": "3.0.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-start": "1.0.1",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "gulp-watch": "5.0.0",
    "gulp-zip": "4.1.0",
    "husky": "0.14.3",
    "less": "2.7.3",
    "lint-staged": "6.1.0",
    "main-bower-files": "2.13.1",
    "prettier": "1.10.2",
    "require-dir": "1.0.0",
    "uglify-js": "3.3.9",
    "yargs-parser": "9.0.2"
  },
  "license": "MIT",
  "name": "gae-init",
  "repository": {
    "type": "git",
    "url": "https://github.com/gae-init"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"],
    "*.less": ["prettier --write", "git add"],
    "*.md": ["prettier --write", "git add"]
  },
  "scripts": {
    "build": "gulp build",
    "fix:config": "prettier --ignore-path .gitignore --write \"**/*.json\"",
    "fix:docs": "prettier --ignore-path .gitignore --write \"**/*.md\"",
    "fix:script": "npm run test:script -- --fix",
    "fix:style": "prettier --ignore-path .gitignore --write \"**/*.less\"",
    "fix": "npm run fix:script && npm run fix:config && npm run fix:style && npm run fix:docs",
    "install": "gulp init",
    "postinstall": "echo 'Run `gulp` to start or `gulp help` for more.'",
    "precommit": "lint-staged",
    "start": "gulp",
    "test:script": "eslint --ignore-path .gitignore \"**/*.js\"",
    "test": "npm run test:script"
  }
}
